<h2>Move Details</h2>

<h4>{{title}}</h5>
<h6>{{genre}}</h6>
<p>{{plot}}</p>

{{#each cast}}
<h5>Cast-Details</h5>
  <p>Name: {{name}}</p>
  <p>Occupation: {{occupation}}</p>
  <p>Catch Phrase:{{catchPhrase}}</p>
{{/each}}

<form action="/movies/{{_id}}/delete" method="post">
<button>Delete</button>

<a href="/movies/{{_id}}/edit">Edit Movie</a>

</form>